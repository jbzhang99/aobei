#优惠环境配置
type CouponEnv{

    coupon_env_id : Long

    #名称
    name : String

    #适用条件类型  1 对新注册用户优惠
    type : Int

    #适用条件的对应条件    类型1  {"sdate":"2018-02-10 00:00:00","edate":"2018-02-20 00:00:00"}
    condition_env : String

    #优惠券数量
    coupon_number : Int

    #优惠卷id
    coupon_id : Long

    #开始时间
    start_datetime : Date

    #结束时间
    end_datetime : Date

    #优惠策略创建时间
    create_datetime : Date

    #0 无效，1 有效，3已过期
    status : Int
}

