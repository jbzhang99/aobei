#顾客
type Customer{

    #顾客id
    customer_id : Long

    #名称
    name : String

    #用户头像 图片JSON 数据 {id:图片id,url:“图片访问url”}
    logo_img : String

    #性别 0 未知  1 男 2 女
    gender : Int

    #手机号
    phone : String

    #身份证号
    idcard : String

    #创建时间
    create_datetime : Date

    #用户是否锁定  0 正常 1 锁定  
    locked : Int

    #用户id
    user_id : Long

    #渠道id
    channel_id : Int
}

