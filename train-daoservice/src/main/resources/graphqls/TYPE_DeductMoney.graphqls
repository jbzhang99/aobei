#扣款申请表
type DeductMoney{

    #扣款单id
    deduct_money_id : Long

    #订单id
    pay_order_id : String

    #服务单id
    serviceunit_id : Long

    #扣款金额
    deduct_amount : Int

    #扣款原因
    deduct_info : String

    #申请时间
    create_datetime : Date

    #扣款申请人
    deduct_operator : Long

    #状态   1 待处理 2 完成 3 驳回
    deduct_status : Int

    #确认时间
    confirm_datetime : Date

    #确认人
    confirm_operator : Long
}

