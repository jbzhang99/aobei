    #服务人员ID
    student_id:Long
    #服务人员姓名
    student_name:String
    #服务人员电话
    student_phone:String
    #合伙人ID
    partner_id:Long
    #合伙人姓名
    partner_name:String
    #合伙人电话
    partner_phone:String
